{
	"name": "@public-ui-/theme-bzst",
	"version": "2.0.0-rc.11",
	"private": true,
	"scripts": {
		"depcheck": "depcheck --ignore-bin-package --skip-missing --ignores=@types/*",
		"format": "prettier --check src",
		"lint": "eslint src && tsc --noemit",
		"xtest": "THEME_MODULE=../dist THEME_EXPORT=BZSt kolibri-visual-test",
		"test-update": "THEME_MODULE=../dist THEME_EXPORT=BZSt kolibri-visual-test --update-snapshots theme-snapshots.spec.js",
		"build": "pnpm --filter=\"@public-ui/themes\" build",
		"prextest": "pnpm build",
		"pretest-update": "pnpm build"
	},
	"devDependencies": {
		"@public-ui/schema": "2.0.0-rc.11",
		"@public-ui/visual-tests": "2.0.0-rc.11",
		"@types/node": "ts5.3",
		"typescript": "5.3.2"
	},
	"peerDependencies": {
		"@public-ui/components": "2.0.0-rc.11"
	}
}
